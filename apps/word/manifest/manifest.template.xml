<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
           xsi:type="TaskPaneApp">
  <Id>__GUID__</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Train360, Corp.</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="ProjDocs"/>
  <Description DefaultValue="ProjDocs Connector"/>
  <IconUrl DefaultValue="__DOMAIN__/icons/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="__DOMAIN__/icons/64-icon.png"/>
  <SupportUrl DefaultValue="https://projdocs.com"/>

  <AppDomains>
    <AppDomain>__DOMAIN__</AppDomain>
  </AppDomains>

  <Hosts>
    <Host Name="Document"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="SharedRuntime" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <DefaultSettings>
    <SourceLocation DefaultValue="__DOMAIN__/src/surfaces/ribbon/index.html"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

  <VersionOverrides
      xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides"
      xsi:type="VersionOverridesV1_0"
  >

    <Hosts>
      <Host xsi:type="Document">

        <Runtimes>
          <Runtime resid="Commands.Url" lifetime="long"/>
        </Runtimes>

        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">

            <CustomTab id="__TAB_ID__">

              <Group id="__GROUP_A_ID__">
                <Label resid="__GROUP_A_ID__.L"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="__RETURN_DOC_BTN_ID__">
                  <Label resid="__GROUP_A_ID__.__RETURN_DOC_BTN_ID__.L"/>
                  <Supertip>
                    <Title resid="__GROUP_A_ID__.__RETURN_DOC_BTN_ID__.L"/>
                    <Description resid="__GROUP_A_ID__.__RETURN_DOC_BTN_ID__.D"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.CheckIn.16x16"/>
                    <bt:Image size="32" resid="Icon.CheckIn.32x32"/>
                    <bt:Image size="80" resid="Icon.CheckIn.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>__RETURN_DOC_FUNC_ID__</FunctionName>
                  </Action>
                  <Enabled>false</Enabled>
                </Control>

              </Group>

              <!-- MAIN COMMAND GROUP -->
              <Group id="__GROUP_B_ID__">
                <Label resid="__GROUP_B_ID__.L"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>

                <!-- Button 1: Save -->
                <Control xsi:type="Button" id="__SAVE_BUTTON_ID__">
                  <Label resid="__SAVE_BUTTON_ID__.Label"/>
                  <Supertip>
                    <Title resid="__SAVE_BUTTON_ID__.Label"/>
                    <Description resid="__SAVE_BUTTON_ID__.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.Save.16x16"/>
                    <bt:Image size="32" resid="Icon.Save.32x32"/>
                    <bt:Image size="80" resid="Icon.Save.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>__SAVE_FUNC_ID__</FunctionName>
                  </Action>
                  <Enabled>false</Enabled>
                </Control>

                <!-- Button 2: Save As New Version -->
                <Control xsi:type="Button" id="__SAVE_NEW_VERSION_BUTTON_ID__">
                  <Label resid="__SAVE_NEW_VERSION_BUTTON_ID__.Label"/>
                  <Supertip>
                    <Title resid="__SAVE_NEW_VERSION_BUTTON_ID__.Label"/>
                    <Description resid="__SAVE_NEW_VERSION_BUTTON_ID__.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.SaveAsNewVersion.16x16"/>
                    <bt:Image size="32" resid="Icon.SaveAsNewVersion.32x32"/>
                    <bt:Image size="80" resid="Icon.SaveAsNewVersion.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>__SAVE_NEW_VERSION_FUNC_ID__</FunctionName>
                  </Action>
                  <Enabled>false</Enabled>
                </Control>

                <!-- Button 3: Save As New Document -->
                <Control xsi:type="Button" id="__SAVE_NEW_DOC_BUTTON_ID__">
                  <Label resid="__SAVE_NEW_DOC_BUTTON_ID__.Label"/>
                  <Supertip>
                    <Title resid="__SAVE_NEW_DOC_BUTTON_ID__.Label"/>
                    <Description resid="__SAVE_NEW_DOC_BUTTON_ID__.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.SaveAsNewDocument.16x16"/>
                    <bt:Image size="32" resid="Icon.SaveAsNewDocument.32x32"/>
                    <bt:Image size="80" resid="Icon.SaveAsNewDocument.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>__SAVE_NEW_DOC_FUNC_ID__</FunctionName>
                  </Action>
                  <Enabled>true</Enabled>
                </Control>
              </Group>

              <Group id="__GROUP_C_ID__">
                <Label resid="__GROUP_C_ID__.L"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="__INSERT_DOC_ID_BUTTON_ID__">
                  <Label resid="__GROUP_C_ID__.M.L"/>
                  <Supertip>
                    <Title resid="__GROUP_C_ID__.M.L"/>
                    <Description resid="__GROUP_C_ID__.M.T"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.InsertDocID.16"/>
                    <bt:Image size="32" resid="Icon.InsertDocID.32"/>
                    <bt:Image size="80" resid="Icon.InsertDocID.80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>__INSERT_DOC_ID_FUNC__</FunctionName>
                  </Action>
                  <Enabled>false</Enabled>
                </Control>
              </Group>

              <Label resid="__TAB_ID__.Label"/>
            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="__DOMAIN__/icons/16-icon.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="__DOMAIN__/icons/32-icon.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="__DOMAIN__/icons/80-icon.png"/>

        <bt:Image id="Icon.CheckIn.16x16" DefaultValue="__DOMAIN__/icons/check-in-16.png"/>
        <bt:Image id="Icon.CheckIn.32x32" DefaultValue="__DOMAIN__/icons/check-in-32.png"/>
        <bt:Image id="Icon.CheckIn.80x80" DefaultValue="__DOMAIN__/icons/check-in-80.png"/>

        <!-- Button 1: Save -->
        <bt:Image id="Icon.Save.16x16" DefaultValue="__DOMAIN__/icons/save-16.png"/>
        <bt:Image id="Icon.Save.32x32" DefaultValue="__DOMAIN__/icons/save-32.png"/>
        <bt:Image id="Icon.Save.80x80" DefaultValue="__DOMAIN__/icons/save-80.png"/>

        <!-- Button 2: Save As New Version -->
        <bt:Image id="Icon.SaveAsNewVersion.16x16" DefaultValue="__DOMAIN__/icons/new-version-16.png"/>
        <bt:Image id="Icon.SaveAsNewVersion.32x32" DefaultValue="__DOMAIN__/icons/new-version-32.png"/>
        <bt:Image id="Icon.SaveAsNewVersion.80x80" DefaultValue="__DOMAIN__/icons/new-version-80.png"/>

        <!-- Button 3: Save As New Document -->
        <bt:Image id="Icon.SaveAsNewDocument.16x16" DefaultValue="__DOMAIN__/icons/new-file-16.png"/>
        <bt:Image id="Icon.SaveAsNewDocument.32x32" DefaultValue="__DOMAIN__/icons/new-file-32.png"/>
        <bt:Image id="Icon.SaveAsNewDocument.80x80" DefaultValue="__DOMAIN__/icons/new-file-80.png"/>

        <!-- Insert Doc ID Button -->
        <bt:Image id="Icon.InsertDocID.16" DefaultValue="__DOMAIN__/icons/doc-id-16.png"/>
        <bt:Image id="Icon.InsertDocID.32" DefaultValue="__DOMAIN__/icons/doc-id-32.png"/>
        <bt:Image id="Icon.InsertDocID.80" DefaultValue="__DOMAIN__/icons/doc-id-80.png"/>

      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Commands.Url" DefaultValue="__DOMAIN__/src/surfaces/ribbon/index.html"/>
      </bt:Urls>
      <bt:ShortStrings>

        <bt:String id="GetStarted.Title" DefaultValue="ProjDocs loaded successfully!"/>
        <bt:String id="__TAB_ID__.Label" DefaultValue="ProjDocs"/>

        <bt:String id="__GROUP_A_ID__.L" DefaultValue="Actions"/>
        <bt:String id="__GROUP_A_ID__.__RETURN_DOC_BTN_ID__.L" DefaultValue="Check-In Document"/>

        <bt:String id="__GROUP_B_ID__.L" DefaultValue="Save"/>
        <bt:String id="__SAVE_BUTTON_ID__.Label" DefaultValue="Save Version"/>
        <bt:String id="__SAVE_NEW_VERSION_BUTTON_ID__.Label" DefaultValue="Save as New Version"/>
        <bt:String id="__SAVE_NEW_DOC_BUTTON_ID__.Label" DefaultValue="Save as New Document"/>

        <bt:String id="__GROUP_C_ID__.L" DefaultValue="Tools"/>
        <bt:String id="__GROUP_C_ID__.M.L" DefaultValue="Insert Document ID"/>

      </bt:ShortStrings>
      <bt:LongStrings>

        <bt:String
            id="GetStarted.Description"
            DefaultValue="The ProjDocs add-in loaded successfully. Go to the HOME tab to continue."
        />

        <bt:String id="__GROUP_A_ID__.__RETURN_DOC_BTN_ID__.D" DefaultValue="Return the document and allow others to edit."/>


        <!-- Button 1: Save-->
        <bt:String
            id="__SAVE_BUTTON_ID__.Tooltip"
            DefaultValue="Save the document, overwriting the current version."
        />

        <!-- Button 2: Save As New Version -->
        <bt:String
            id="__SAVE_NEW_VERSION_BUTTON_ID__.Tooltip"
            DefaultValue="Save the document as a new version."
        />

        <!-- Button 3: Save As New Document -->
        <bt:String
            id="__SAVE_NEW_DOC_BUTTON_ID__.Tooltip"
            DefaultValue="Save the document as a new file."
        />

        <bt:String id="__GROUP_C_ID__.M.T" DefaultValue="Insert the document's id at the current cursor position."/>


      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>